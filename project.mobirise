          "_name": "design-block",
          "_customHTML": "<section mbr-id=\"_anchor\" class=\"_customHTML\">\n \n  \n    <mbr-parameters>\n<hr><header>Whatsapp Config</header>\n      <input type=\"checkbox\" title=\"Whatsapp Button\" name=\"Whatbutton\" checked>\n      <input type=\"text\" name=\"Phonenumber\" title=\"Phone number\" value=\"+5548999333488\" condition=\"Whatbutton\">\n      <input type=\"text\" name=\"Textewhatapp\" title=\"Your Text\" value=\"Oi! Gostaria de mais informações\" condition=\"Whatbutton\">\n        <input type=\"range\" inline title=\"Height\" name=\"heightwhat\" min=\"0\" max=\"100\" step=\"1\" value=\"60\" condition=\"Whatbutton\">\n        <input type=\"range\" inline title=\"Width\" name=\"widthwhat\" min=\"0\" max=\"100\" step=\"1\" value=\"60\" condition=\"Whatbutton\">\n              <input type=\"range\" inline title=\"Bottom\" name=\"bottomwhat\" min=\"0\" max=\"250\" step=\"1\" value=\"63\" condition=\"Whatbutton\">\n              <input type=\"range\" inline title=\"Right\" name=\"rightwhat\" min=\"0\" max=\"250\" step=\"1\" value=\"100\" condition=\"Whatbutton\">\n        <input type=\"range\" inline title=\"border-radius\" name=\"borderadius\" min=\"1\" max=\"100\" step=\"1\" value=\"60\" condition=\"Whatbutton\">\n        <input type=\"color\" title=\"Background Color\" name=\"bgcolorwhat\" value=\"#25d366\" condition=\"Whatbutton\">\n        <input type=\"color\" title=\"Color\" name=\"colorwhat\" value=\"#ffffff\" condition=\"Whatbutton\">\n      <input type=\"range\" title=\"Font Size\" name=\"FontSize\" min=\"0\" max=\"100\" step=\"1\" value=\"30\" condition=\"Whatbutton\">\n            \n    </mbr-parameters> \n  <div class=\"whatapp\" mbr-if=\"Whatbutton\">\n\t\t<link rel=\"stylesheet\" href=\"https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css\">\n<a href=\"https://api.whatsapp.com/send?phone={{Phonenumber}}&text={{Textewhatapp}}\" class=\"float\" target=\"_blank\">\n<i class=\"fa fa-whatsapp my-float\"></i>\n</a>\n\t</div>